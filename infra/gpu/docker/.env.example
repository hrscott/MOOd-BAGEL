# Environment variables consumed by docker/compose.yaml
# Copy this file to .env and adjust for your environment.

# The absolute path on the host where design results will be persisted.
RESULTS_DIR=/workspace/results

# The absolute path to a directory containing input FASTA or JSON payloads.
INPUT_DIR=/workspace/input

# Optional path to a cache for large model weights (e.g., AlphaFold params).
MODEL_CACHE=/workspace/model-cache
